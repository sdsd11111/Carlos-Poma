{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api/send-email",
      "destination": "/api/send-email.js"
    }
  ],
  "functions": {
    "api/send-email.js": {
      "memory": 128,
      "maxDuration": 5
    }
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}